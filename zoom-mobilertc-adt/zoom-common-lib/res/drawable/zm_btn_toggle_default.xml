<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item  android:state_checked="false" android:state_focused="true"   android:drawable="@drawable/zm_toggle_off_pressed" />
	<item android:state_pressed="false" android:state_checked="false" android:drawable="@drawable/zm_toggle_off_normal" />
	<item android:state_pressed="true" android:state_checked="false" android:drawable="@drawable/zm_toggle_off_pressed" />
	<item  android:state_checked="true" android:state_focused="true"   android:drawable="@drawable/zm_toggle_on_pressed" />
	<item android:state_pressed="false" android:state_checked="true" android:drawable="@drawable/zm_toggle_on_normal" />
	<item android:state_pressed="true" android:state_checked="true" android:drawable="@drawable/zm_toggle_on_pressed" />
</selector>
